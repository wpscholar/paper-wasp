{
  "name": "paper-wasp",
  "author": "Micah Wood <micah@wpscholar.com> (https://wpscholar.com)",
  "license": "GPL-2.0+",
  "private": true,
  "scripts": {
    "build": "yarn run build:dev && yarn run build:prod",
    "build:dev": "webpack",
    "build:prod": "NODE_ENV=production webpack -p",
    "dashboard": "yarn run dashboard:dev",
    "dashboard:dev": "DASHBOARD=yes webpack-dashboard webpack --watch",
    "dashboard:prod": "DASHBOARD=yes NODE_ENV=production webpack-dashboard webpack -p --watch",
    "setup": "npm i -g jest webpack yarn && yarn",
    "start": "yarn run dashboard",
    "test": "jest",
    "watch": "yarn run watch:dev",
    "watch:dev": "webpack --watch",
    "watch:prod": "NODE_ENV=production webpack -p --watch"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "modulePaths": [
      "./source/js/modules",
      "./source/js/components",
      "./node_modules"
    ],
    "setupFiles": [
      "./source/js/jest-config.js"
    ],
    "testPathIgnorePatterns": [
      "node_modules"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "babel-preset-env",
            {
              "modules": false,
              "targets": {
                "browsers": [
                  "last 3 versions",
                  ">1%",
                  "not ie < 10",
                  "not OperaMini all"
                ]
              }
            }
          ],
          "babel-preset-react",
          "babel-preset-flow"
        ],
        "plugins": [
          "transform-class-properties",
          "transform-object-rest-spread",
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.4",
    "css-mqpacker": "^5.0.1",
    "empty": "^0.10.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jest": "^19.0.1",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-sorting": "^0.3.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "flow-bin": "^0.45.0",
    "imports-loader": "^0.7.1",
    "jest": "^19.0.2",
    "lodash.clone": "^4.5.0",
    "lodash.difference": "^4.5.0",
    "lodash.filter": "^4.6.0",
    "lodash.find": "^4.6.0",
    "lodash.includes": "^4.3.0",
    "lodash.indexof": "^4.0.5",
    "lodash.isempty": "^4.4.0",
    "lodash.map": "^4.6.0",
    "lodash.max": "^4.0.1",
    "lodash.remove": "^4.7.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "lodash.values": "^4.3.0",
    "lodash.without": "^4.4.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^1.3.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-flow-types": "^0.1.1",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.5.4",
    "react-tinymce-input": "^1.2.1",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "reselect": "^3.0.1",
    "sanitize-html": "^1.14.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.16.1",
    "uuid": "^3.0.1",
    "webpack": "^2.6.1",
    "webpack-dashboard": "^2.0.0",
    "whatwg-fetch": "^2.0.3"
  }
}
